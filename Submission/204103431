//C++ code  for finding sum of ASCII values of my name

#include<iostream>
#include<cstring>
using namespace std;

int main()
{
 string str = "TEKEE SURYA SAI";
  int i,j,sum=0;
	
	for(i=65;i<=90;i++)
	{
		for(j=0;j<str.length();j++)
	{
		if(i==str[j])
        sum =sum+i ;
	  }  
	}
	
	cout<<"sum of the ASCII values of my name is"<<" "<<sum<<endl;
return 0;
}

